<Window x:Class="Panel_Gościa.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Panel_Gościa"
        mc:Ignorable="d"
        Title="Login" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtLogin" HorizontalAlignment="Left" Height="23" Margin="247,119,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtPassword" HorizontalAlignment="Left" Height="23" Margin="249,149,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblLogin" Content="Podaj login:" HorizontalAlignment="Left" Margin="170,117,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblHaslo" Content="Podaj hasło:" HorizontalAlignment="Left" Margin="166,147,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnLogin" Content="Zaloguj" HorizontalAlignment="Left" Margin="443,246,0,0" VerticalAlignment="Top" Width="75" Click="btnLogin_Click"/>
        <Button x:Name="btnAnuluj" Content="Anuluj" HorizontalAlignment="Left" Margin="532,247,0,0" VerticalAlignment="Top" Width="75" Click="btnAnuluj_Click"/>

    </Grid>
</Window>
