<Page x:Class="Panel_Gościa.StronyAdmin.Raporty"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Panel_Gościa.StronyAdmin"
      mc:Ignorable="d" 
      d:DesignHeight="340" d:DesignWidth="410"
      Title="Raporty">

    <Grid Background="#FFD3E6F0">
        <ComboBox x:Name="cbxTyp" HorizontalAlignment="Left" Margin="135,76,0,0" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.873,-0.946" SelectionChanged="cbxTyp_SelectionChanged"/>
        <Button x:Name="btnGeneruj" Content="Generuj" HorizontalAlignment="Left" Margin="158,179,0,0" VerticalAlignment="Top" Width="75" Click="btnGeneruj_Click"/>
        <ComboBox x:Name="cbxCzas" HorizontalAlignment="Left" Margin="135,123,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="cbxCzas_SelectionChanged"/>

    </Grid>
</Page>
